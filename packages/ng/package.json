{
  "name": "@mezzanine-ui/ng",
  "version": "0.3.0",
  "description": "Angular components for mezzanine-ui",
  "author": "Mezzanine",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mezzanine-UI/mezzanine.git",
    "directory": "packages/ng"
  },
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "ng test --coverage",
    "build": "node ../../scripts/prepareBuild.js && ng build",
    "build:clean": "node ../../scripts/cleanBuild.js",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d . --disableLifeCycleHooks --disablePrivate --disableProtected",
    "storybook": "yarn docs:json && start-storybook -p 6006",
    "storybook:build": "yarn docs:json && build-storybook"
  },
  "peerDependencies": {
    "@angular/animations": "^11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/forms": "^11.1.1",
    "rxjs": "^6.6.3",
    "zone.js": "^0.11.3"
  },
  "dependencies": {
    "@mezzanine-ui/core": "^0.3.0",
    "@mezzanine-ui/icons": "^0.3.0",
    "@mezzanine-ui/system": "^0.1.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.1.1",
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular-eslint/eslint-plugin": "^2.0.2",
    "@angular-eslint/eslint-plugin-template": "^2.0.2",
    "@angular-eslint/template-parser": "^2.0.2",
    "@angular/animations": "~11.2.6",
    "@angular/cdk": "~11.2.5",
    "@angular/cli": "~11.2.5",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/angular": "^6.2.9",
    "@testing-library/angular": "^10.4.1",
    "jest-preset-angular": "^8.4.0",
    "ng-packagr": "^11.2.4",
    "rxjs": "^6.6.6",
    "zone.js": "^0.11.4"
  }
}
